{
  "name": "subtitler",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "suffix": ".res.mjs",
  "bs-dependencies": [
    "@rescript/core",
    "@rescript/react",
    "rescript-classnames",
    "rescript-webapi",
    "rescript-debounce-react"
  ],
  "bsc-flags": ["-open RescriptCore"],
  "jsx": {
    "version": 4
  },
  "gentypeconfig": {
    "module": "esmodule",
    "moduleResolution": "node",
    "generatedFileExtension": ".gen.tsx"
  }
}
